{
  "manifest_version": 2,
  "name": "Redirect2WppWeb",
  "version": "1.0",
  "description": "Detecta n√∫meros de celular e cria atalhos para o WhatsApp Web.",
  "permissions": ["contextMenus", "storage", "tabs", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Redirect2WppWeb",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
